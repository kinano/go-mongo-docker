{
    "AWSEBDockerrunVersion": 2,
    "authentication": {
        "bucket": "skedaddle-docker-hub-credentials",
        "key": "docker-config.json"
      },
    "containerDefinitions": [
        {
            "name": "api",
            "image": "skedaddle/sk-next-api",
            "hostname": "api",
            "essential": true,
            "memory": 2000
        },
        {
            "name": "nginx",
            "image": "skedaddle/api-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "api"
            ],
            "memory": 2000
        }
    ]
}